modules = ["python-3.11", "postgresql-16", "nodejs-20"]
[agent]
expertMode = true
integrations = ["google-sheet:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "webserver"

[[workflows.workflow]]
name = "webserver"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 server.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[nix]
channel = "stable-25_05"
